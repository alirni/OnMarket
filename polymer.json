{
  "entrypoint": "index.html",
  "shell": "src/onmarket-app.html",
  "fragments": [
    "src/onmarket-config.html",
    "src/onmarket-icon.html",
    "src/onmarket-menu.html",
    "src/onmarket-style.html",
    "src/page-404.html",
    "src/page-cart.html",
    "src/page-detail.html",
    "src/page-game.html",
    "src/page-product.html",
    "src/page-signin.html",
    "src/page-startgame.html",
    "src/product-card-cart.html",
    "src/product-card.html"
  ],
  "sources": [
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js",
    "bower_components/paper-styles/font/*"
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ],
    "ignoreWarnings": []
  },
  "builds": [
    {
      "name": "es6-unbundled",
      "browserCapabilities": [
        "es2015",
        "push"
      ],
      "js": {
        "minify": false,
        "compile": false
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      },
      "addServiceWorker": true,
      "preset": "es6-unbundled"
    }
  ]
}
